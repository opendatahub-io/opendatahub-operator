{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:recommended"
  ],
  "branchPrefix": "renovate/",
  "baseBranches": ["non-existing-branch"],
  "automerge": false,
  "packageRules": [
    {
      "matchUpdateTypes": ["*"],
      "enabled": false
    },
    {
      "matchFileNames": ["*"],
      "enabled": false
    },
    {
      "matchManagers": ["*"],
      "enabled": false
    }
  ],
 "prHourlyLimit": 1
}
